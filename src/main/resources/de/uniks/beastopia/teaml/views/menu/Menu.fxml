      fx:controller="de.uniks.beastopia.teaml.controller.menu.MenuController">
   <HBox fx:id="menuContainer" alignment="CENTER" prefHeight="10000.0">
      <VBox fx:id="regionContainer" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="350.0" prefWidth="300.0"
