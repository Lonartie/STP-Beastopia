<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>
<HBox fx:id="ingame" alignment="BOTTOM_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" onKeyPressed="#pauseMenu" prefHeight="600.0" prefWidth="750.0"
      xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="de.uniks.beastopia.teaml.controller.IngameController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%pressESCToPause">
         <HBox.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
         </HBox.margin>
      </Text>
   </children>
</HBox>
